{
  "name": "demo-app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch",
    "mon-watch": "nodemon --exec electron .",
    "build": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app_icon/logo.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron App\"",
    "package-mac": "electron-packager . --out=dist/osx --platform=darwin --arch=x64",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.apps.app",
    "asar": false,
    "productName": "Test App",
    "target": [
      "deb",
      "snap",
      "rmp"
    ],
    "directories": {
      "output": "./ios-build"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Aazam <khan.azam5253@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.2",
    "css-loader": "^6.2.0",
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.3.0",
    "electron-wix-msi": "^3.2.0",
    "hls": "^0.0.1",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-firebase-auth-hooks": "^1.0.0",
    "react-hls-player": "^3.0.7",
    "react-player": "^2.9.0",
    "react-youtube": "^7.13.1",
    "sass": "^1.37.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "electron-remote": "^1.3.0",
    "electron-updater": "^4.3.9",
    "jspdf": "^2.3.1",
    "make": "^0.8.1",
    "mocha": "^9.0.3",
    "puppeteer": "^10.2.0",
    "react-router-dom": "^5.2.0",
    "sweetalert2": "^11.1.3",
    "symlinks": "^1.0.0"
  }
